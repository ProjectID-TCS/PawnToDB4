<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.pawntodb4app.AddPlayerController">
    <Label text="Dodaj Gracza"/>
    <TextField fx:id="firstNameField" promptText="Imię"/>
    <TextField fx:id="lastNameField" promptText="Nazwisko"/>
    <TextField fx:id="eloField" promptText="ELO"/>
    <Button text="Dodaj" onAction="#handleAddButton"/>
    <Button fx:id="backButton" text="Wróć do Menu Głównego" onAction="#handleBackButton"/>
</VBox>
